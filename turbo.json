{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT",
        ".env*"
      ],
      "outputs": [
        ".svelte-kit/**",
        ".svelte-kit/cloudflare/**",
        ".vercel/**"
      ],
      "env": [
        "PUBLIC_*",
        "PRIVATE_*"
      ]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}